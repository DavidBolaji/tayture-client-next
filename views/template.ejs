                          -moz-border-radius: 0px;
                        "
                      ><!--<![endif]-->
                        <table
                          style="font-family: arial, helvetica, sans-serif"
                          role="presentation"
                          cellpadding="0"
                          cellspacing="0"
                          width="100%"
                          border="0"
                        >
                          <tbody>
                            <tr>
                              <td
                                style="
                                  overflow-wrap: break-word;
                                  word-break: break-word;
                                  padding: 10px;
                                  font-family: arial, helvetica, sans-serif;
                                "
                                align="left"
                              >
                                <div>
                                  <!DOCTYPE html>
                                  <html lang="en">
                                    <head>
                                      <meta charset="UTF-8" />
                                      <meta
                                        name="viewport"
                                        content="width=device-width, initial-scale=1.0"
                                      />
                                      <link
                                        rel="stylesheet"
                                        href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
                                      />
                                      <script src="https://cdn.tailwindcss.com"></script>

                                      <title>Your Resume</title>
                                    </head>
                                    <body>
                                      <div
                                        style="
                                          width: 100%;
                                          display: flex;
                                          flex-direction: row;
                                          border-radius: 0.375rem;
                                          box-shadow: 0px 2px 4px
                                            rgba(0, 0, 0, 0.1);
                                          margin-top: -30px;
                                        "
                                      >
                                        <div
                                          style=<%"
                                            background-color:" colorList.foreground";"
                                            "flex: 7;"
                                            "padding: 1.25rem;"
                                            "white-space: pre-line;"
                                          %>
                                        >
                                          <div
                                            style=<%="
                                              font-size: 1.125rem;"
                                              "font-weight: bold;"
                                              "text-transform: uppercase;"
                                              "color:" colorList.background";"
                                             " border-bottom: 1.5px solid"
                                                colorList.background";"
                                              
                                            "font-size: 40px;"
                                              "line-height: 45px;"
                                            %>
                                          >
                                            <%= data.name %>
                                          </div>

                                          <h2
                                            style=<%="
                                              margin-top: 1.25rem;"
                                              "margin-bottom: 0.5rem;"
                                              "font-size: 1.5rem;"
                                             " font-weight: bold;"
                                              "text-transform: uppercase;"
                                              "color:"colorList.background";"
                                              "border-bottom: 1px solid "colorList.background";"
                                              "margin-top: -18px;"
                                            %>
                                          >
                                            Professional Summary
                                          </h2>

                                          <div
                                            style=<%="
                                              font-size: 0.875rem;"
                                              "color: "colorList.colorParagraph";"
                                              "margin-top: -38px;"
                                            %>
                                          >
                                            <%= data.summary %>
                                          </div>
                                          <div style="margin-top: 1.25rem">
                                            <h2
                                              style=<%="
                                                font-size: 1.5rem;"
                                               " font-weight: bold;"
                                                "text-transform: uppercase;"
                                                "color: "colorList.background";"
                                                "border-bottom: 1px solid "colorList.background";"
                                              %>
                                            >
                                              Work History
                                            </h2>
                                            <div></div>
                                            <!-- Work History Content -->
                                            <% data.history.forEach((d, i) => { %>
                                            <div style="margin-bottom: 10px">
                                              <div
                                                style="
                                                  display: flex;
                                                  justify-content: space-between;
                                                  align-items: center;
                                                  margin-top: -115px;
                                                "
                                              >
                                                <p
                                                  style="
                                                    font-weight: 700;
                                                    max-width: 300px;
                                                    text-transform: uppercase;
                                                    font-size: 13px;
                                                  "
                                                >
                                                  <%= d.title %>
                                                </p>
                                                <p style="font-size: 10px">
                                                  <%= d.date %>
                                                </p>
                                              </div>
                                              <p
                                                style="
                                                  font-weight: 700;
                                                  margin-top: -20px;
                                                  font-size: 13px;
                                                "
                                              >
                                              <%= d.location %>
                                              </p>
                                              <ul
                                                style="
                                                  margin-left: -28px;
                                                  margin-top: -15px;
                                                  font-size: 12px;
                                                  margin-top: -32px;
                                                "
                                              >
                                              <% d.roles.forEach((k, i) => { %>
                                                <li
                                                  style="margin-bottom: -15px"
                                                >
                                                 <%= k.role %>
                                                </li>
                                               
                                                <%}) %>
                                              </ul>
                                            </div>
                                            <%}) %>
                                          </div>
                                          <div style="margin-top: 1.25rem">
                                            <h2
                                              style=<%=
                                                "font-size: 1.5rem;"
                                                "font-weight: bold;"
                                                "text-transform: uppercase;"
                                                "color: "colorList.background";"
                                               " margin-top: -100px;"
                                                "border-bottom: 1px solid " colorList.background";"
                                              %>
                                            >
                                              Education
                                            </h2>
                                            <!-- Education Content -->
                                            <% data.education.forEach((e, i) => { %>
                                            <div>
                                              <div
                                                style="
                                                  display: flex;
                                                  justify-content: space-between;
                                                  align-items: center;
                                                  margin-top: -70px;
                                                "
                                              >
                                                <p
                                                  style="
                                                    font-weight: 700;
                                                    max-width: 300px;
                                                    text-transform: uppercase;
                                                    font-size: 13px;
                                                  "
                                                >
                                                  <%= e.degree %>
                                                </p>
                                                <p style="font-size: 10px">
                                                  <%= e.year %>
                                                </p>
                                              </div>
                                              <p
                                                style="
                                                  font-weight: 700;
                                                  margin-top: -20px;
                                                  font-size: 13px;
                                                "
                                              >
                                              <%= e.school %>
                                              </p>
                                            </div>
                                            <%}) %>
                                          </div>
                                        </div>
                                        <div
                                          style=<%=
                                            "flex: 3;"
                                            "padding: 1.25rem;"
                                            "display: flex;"
                                            "flex-direction: column;"
                                            "align-items: flex-start;"
                                            "background-color: "colorList.background";"
                                            "margin-top: 30px;"
                                          %>
                                        >
                                          <div
                                            style="
                                              display: flex;
                                              gap: 0.75rem;
                                              align-items: center;
                                            "
                                          >
                                            <div
                                              style=<%=
                                                "width: 1.75rem;"
                                                "height: 1.5rem;"
                                                "border-radius: 9999px;"
                                                "background-color: "colorList.textOne";"
                                                "display: flex;"
                                                "justify-content: center;"
                                                "align-items: center;"
                                              %>
                                            >
                                              <!-- Location Icon -->
                                              <div
                                                style="
                                                  margin-left: -2px;
                                                  background-color: red;
                                                  width: 20px;
                                                  display: flex;
                                                  align-items: center;
                                                  justify-content: center;
                                                  height: 20px;
                                                  border-radius: 50%;
                                                "
                                              >
                                                <i class="fa fa-map-marker"></i>
                                              </div>
                                            </div>
                                            <div
                                              style=<%="
                                                font-size: 10px;"
                                                "color: "colorList.textOne";"
                                              %>
                                            >
                                              <%=data.location %>
                                            </div>
                                          </div>
                                          <div
                                            style="
                                              display: flex;
                                              gap: 0.75rem;
                                              align-items: center;
                                            "
                                          >
                                            <div
                                              style=<%=
                                                "width: 1.5rem;"
                                                "height: 1.5rem;"
                                                "border-radius: 9999px;"
                                               " background-color:" colorList.textOne";"
                                                "display: flex;"
                                                "justify-content: center;"
                                                "align-items: center;"
                                              %>
                                            >
                                              <!-- Phone Icon -->
                                              <div
                                                style=<%=
                                                  "background-color:" colorList.textOne";"
                                                  "width: 20px;"
                                                  "display: flex;"
                                                  "align-items: center;"
                                                  "justify-content: center;"
                                                  "height: 20px;"
                                                  "border-radius: 50%;"
                                                %>
                                              >
                                                <i class="fa fa-phone"></i>
                                              </div>
                                            </div>
                                            <div
                                              style=<%=
                                                "font-size: 10px;"
                                                "color: " colorList.textOne";"
                                              %>
                                            >
                                              <%=data.number %>
                                            </div>
                                          </div>
                                          <div
                                            style=<%=
                                              "display: flex;"
                                              "gap: 0.75rem;"
                                              "align-items: center;"
                                            %>
                                          >
                                            <div
                                              style=<%=
                                                "width: 1.75rem;"
                                                "height: 1.5rem;"
                                                "border-radius: 9999px;"
                                                "background-color: "colorList.textOne";"
                                                "display: flex;"
                                                "justify-content: center;"
                                                "align-items: center;"
                                              %>
                                            >
                                              <!-- Email Icon -->
                                              <div
                                                style="
                                                  background-color: red;
                                                  width: 20px;
                                                  display: flex;
                                                  align-items: center;
                                                  justify-content: center;
                                                  height: 20px;
                                                  border-radius: 50%;
                                                "
                                              >
                                                <i class="fa fa-envelope-o"></i>
                                              </div>
                                            </div>
                                            <div
                                              style=<%=
                                                "font-size: 10px;"
                                                "color:" colorList.textOne";"
                                              %>
                                            >
                                              <%= data.email %>
                                            </div>
                                          </div>
                                          <div style="margin-top: 1.25rem">
                                            <h2
                                              style=<%=
                                               "font-size: 1.5rem;"
                                                "font-weight: bold;"
                                                "text-transform: uppercase;"
                                                "color: "colorList.textOne";"
                                                "border-bottom: 1px solid" black;
                                              %>
                                            >
                                              Skills
                                            </h2>
                                            <ul
                                              style="
                                                margin-left: -38px;
                                                list-style: none;
                                                margin-top: -15px;
                                                font-size: 12px;
                                              "
                                            >
                                            <% data.education.forEach((r, i) => { %>
                                                <li> <%= r %></li>
                                              <%}) %>

                                            </ul>
                                          </div>
                                        </div>
                                      </div>
                                    </body>
                                  </html>
                                </div>
                              </td>
                            </tr>
                          </tbody>
                        </table>

                        <!--[if (!mso)&(!IE)]><!-->
                      </div>
                      <!--<![endif]-->
                    </div>
                  </div>
                  <!--[if (mso)|(IE)]></td><![endif]-->
                  <!--[if (mso)|(IE)]></tr></table></td></tr></table><![endif]-->
                </div>
              </div>
            </div>

            <!--[if (mso)|(IE)]></td></tr></table><![endif]-->
          </td>
        </tr>
